{
  "name": "twoslashes",
  "type": "module",
  "version": "0.0.0-1",
  "description": "Rewrite of TypeScript TwoSlash",
  "author": "TypeScript team",
  "license": "MIT",
  "homepage": "https://github.com/antfu/twoslashes",
  "repository": {
    "url": "https://github.com/antfu/twoslashes",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/antfu/twoslashes/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "unbuild",
    "test": "vitest",
    "lint": "eslint .",
    "bench": "vitest bench --run",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "dependencies": {
    "@typescript/vfs": "1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@typescript/twoslash": "^3.2.4",
    "eslint": "^8.56.0",
    "fast-glob": "^3.3.2",
    "lz-string": "^1.5.0",
    "ohash": "^1.1.3",
    "react": "^18.2.0",
    "tslib": "^1.14.1",
    "twoslashes": "workspace:*",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.3"
  }
}
